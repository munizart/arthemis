{
  "name": "arthemis",
  "version": "1.0.3",
  "description": "Simple API decoupled dependency injection",
  "main": "index.js",
  "scripts": {
    "test": "npx jest",
    "build": "npm run generateDoc && npm run lint && npm run dist",
    "dist": "npx babel --plugins transform-es2015-modules-umd ./src/Container.js > ./index.js",
    "lint": "eslint --ext src/*.js ./",
    "generateDoc": "npx jsdoc2md src/*.js > ./docs/README.md"
  },
  "keywords": [],
  "author": "munizart",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jsdoc-to-markdown": "^4.0.1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1"
  }
}
